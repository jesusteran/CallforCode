{
  "name": "YOUR_CHROME_APP_NAME",
  "description": "YOUR_CHROME_APP_DESCRIPTION",
  "manifest_version": 3,
  "version": "1.0.1",
  "background": {
    "persistent": false,
    "scripts": ["BMSPushSDK.js","background.js"]
  },
  "browser_action": {
    "default_title": "Bluemix Example",
    "default_icon": "icon-128.png",
    "default_popup": "window.html"
  },
  "permissions": ["gcm", "storage", "notifications","*://*.cloud.ibm.com/*","identity"],
  "icons": { "128": "icon-128.png" }
}
